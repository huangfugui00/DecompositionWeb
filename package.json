{
  "private": true,
  "name": "decomposition",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "electron": "electron .",
    "exe": "concurrently -k \"npm start\" \"npm:electron\"",
    "electron1": "wait-on tcp:3000 && electron .",
    "electron_builder": "electron-builder --win --x64",
    "dist": "concurrently -k \"npm start\" \"npm:electron_builder\" "
  },
  "homepage": ".",
  "main": "index.js",
  "build": {
    "appId": "com.xxx.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "out"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "@mui/material-next": "^6.0.0-alpha.13",
    "axios": "^0.26.0",
    "concurrently": "^7.0.0",
    "echarts": "^5.3.0",
    "echarts-for-react": "^3.0.2",
    "echarts-gl": "^2.0.9",
    "fs": "0.0.1-security",
    "mathjs": "^10.4.0",
    "netcdfjs": "^2.0.1",
    "next": "latest",
    "plotly": "^1.0.6",
    "plotly.js": "^2.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading": "^2.0.3",
    "react-plotly": "^1.0.0",
    "react-plotly.js": "^2.5.1",
    "react-toastify": "^8.2.0",
    "recharts": "^2.1.9"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-plotly.js": "^2.5.0",
    "autoprefixer": "^10.4.0",
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13",
    "postcss": "^8.4.4",
    "tailwindcss": "^3.0.1",
    "typescript": "^4.5.3"
  }
}
